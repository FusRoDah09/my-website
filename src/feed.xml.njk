---
permalink: /feed.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Darpan Choudhary - Writings</title>
    <link>https://darpanchoudhary.com/</link>
    <language>en-us</language>
    <lastBuildDate>{{ collections.writings[collections.writings.length - 1].date | rssDate }}</lastBuildDate>
    <atom:link href="https://darpanchoudhary.com/feed.xml" rel="self" type="application/rss+xml" />
    {% for post in collections.writings | reverse %}
    <item>
      <title>{{ post.data.title }}</title>
      <link>https://darpanchoudhary.com{{ post.url }}</link>
      <guid isPermaLink="true">https://darpanchoudhary.com{{ post.url }}</guid>
      <pubDate>{{ post.date | rssDate }}</pubDate>
      <description><![CDATA[{{ post.templateContent | safe }}]]></description>
    </item>
    {% endfor %}
  </channel>
</rss>